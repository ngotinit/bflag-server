<div class="container">
    <div class="index">
        <%= link_to "Create", new_admin_user_path, class:"btn btn-info", id: "spacing2"%>
    </div>
    <table class="responsive nowrap table table-hover" role="datatable">
        <thead class="thead-dark">
            <tr>
                <th>ID</th>
                <th>Username</th>
                <!-- <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th> -->
                <th ></th>
            </tr>
        </thead>
        <tbody>
          <%@users.each do |f| %>
          <tr>
            <td><%= f.id%></td>
            <td><%= f.username %></td>
            <td><%= link_to "show" , admin_user_path(f)%>
          </tr>
          <% end %>
        </tbody>
    </table>

</div>


